#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Github/eslint-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Github/eslint-monorepo/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jsdoc@3.6.11/node_modules/jsdoc/jsdoc.js" "$@"
else
  exec node  "$basedir/../../../../../jsdoc@3.6.11/node_modules/jsdoc/jsdoc.js" "$@"
fi
