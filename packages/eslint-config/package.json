{
  "$schema": "https://json.schemastore.org/package",
  "name": "@srmcguirt/eslint-config",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.21.0",
  "description": "ESLint shared config",
  "license": "MIT",
  "repository": "srmcguirt/eslint-config",
  "sideEffects": false,
  "scripts": {
    "build": "node esbuild.js",
    "check": "npx tsc",
    "watch": "nodemon out/index.js",
    "lint": "npx eslint src/**/*.ts"
},
  "dependencies": {
    "@srmcguirt/eslint-repo-shared": "workspace:*"
  },
  "devDependencies": {
    "eslint": "8.31.0",
    "@srmcguirt/eslint-repo-config": "workspace:*",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
