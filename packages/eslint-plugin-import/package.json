{
  "name": "@srmcguirt/eslint-plugin-import",
  "version": "0.0.0",
  "description": "Reformatted import plugin for ESLint",
  "repository": "srmcguirt/eslint-plugin-import",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@eslint/eslintrc": "1.4.1",
    "@types/eslint": "8.4.10",
    "eslint-plugin-import": "2.26.0"
  },
  "node": ">=14.16",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "packageManager": "pnpm@7.21.0"
}
