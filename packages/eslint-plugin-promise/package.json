{
  "name": "@srmcguirt/eslint-plugin-promise",
  "version": "0.0.0",
  "description": "Reformatted promise plugin for ESLint",
  "repository": "srmcguirt/eslint-plugin-promise",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "eslint-plugin-promise": "latest"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "packageManager": "pnpm@7.21.0"
}
